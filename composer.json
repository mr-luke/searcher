{
    "name": "mr-luke/searcher",
    "description": "Laravel 5 package for REST Url-based quering, filtering & sorting Eloquent models.",
    "keywords": ["eloquent", "searching", "REST", "laravel"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Łukasz Sitnicki",
            "email": "lukasz.sitnicki@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/database": "^5.5",
        "illuminate/support": "^5.5"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "orchestra/testbench": "^3.5",
        "phpunit/phpunit": "^7.0",
        "orchestra/database": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Mrluke\\Searcher\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/database/seeds",
            "tests/database/factories"
        ],
        "psr-4": {
            "Mrluke\\Searcher\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mrluke\\Searcher\\SearcherServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
